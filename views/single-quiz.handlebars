<div class="quiz">
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>{{quiz.title}}</h2>
      <div>
        Posted by
        {{quiz.User.email}}
        on
        {{format_date quiz.createdAt}}
      </div>
    </div>
    <div class="card-body">
      {{quiz.body}}
    </div>
  </article>